@article{ WOS:001581247700002,
Author = {Wang, Yujie and Yu, Huahao and Zhang, Xuechen and Song, Xinbei and Ye,
   Sitan and Gouda, Mostafa and Li, Xiaoli},
Title = {Interpretable machine learning-assisted mid-infrared spectroscopy for
   qualitative and quantitative quality assessment of flat green tea},
Journal = {FOOD CONTROL},
Year = {2026},
Volume = {180},
Month = {FEB},
Abstract = {The identification of the geographical origin of tea is a long-standing
   challenge. However, green and recyclable identification methods
   applicable to tea geographic origin are still missing. Therefore, this
   study aims to explore the feasibility of mid-infrared spectroscopy (MIR)
   and machine learning for geographic origin traceability and component
   detection in flat green tea. One hundred flat tea samples with 50
   geographical indication (GI) and 50 non-GI labels of Longjing tea were
   evaluated. Significant differences (p < 0.05) were observed in both tea
   polyphenols and free amino acids between GI and non-GI samples. The
   nonlinear SVM model based on MIR achieved accurate identification for 34
   tea samples with an accuracy of 97.06 \%. The nonlinear SVR and linear
   CARS-PLSR models achieved the quantification of tea polyphenols and
   amino acids with RMSE values of 0.78 \% and 0.09 \%, respectively. The
   SHapley Additive exPlanations (SHAP) values further indicated that the
   wavenumbers at 2196 (negatively contributing), 1988 (positively
   contributing) and 2181 cm(-1) (positively contributing) were dominated
   in predicting the geographical origin, tea polyphenols and amino acids,
   respectively, thereby contributing to the accuracy of the models. This
   study offers a reliable model for the traceability of geographic origin
   in agricultural products.},
