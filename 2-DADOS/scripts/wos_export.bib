@article{ WOS:001500916300001,
Author = {Li, Min and Zhang, Qian and Han, Guilin and Liang, Tao and Liu, Jinrong
   and Wang, Daobing and Zhong, Qiding},
Title = {Multivariate discrimination of Chinese vinegars using multi-element:
   Implications for origin traceability and dietary safety},
Journal = {JOURNAL OF FOOD COMPOSITION AND ANALYSIS},
Year = {2025},
Volume = {145},
Month = {SEP},
Abstract = {The authenticity of geographical indications of famous vinegars is
   crucial for maintaining market order and consumer confidence. In this
   study, 60 vinegar samples were collected from four renowned Chinese
   vinegar, including Shanxi aged vinegar, Baoning vinegar, Zhenjiang
   fragrant vinegar, and Fujian Monascus vinegar, and were analyzed for 18
   multi-mineral elements. Significant variations in elemental composition
   were observed, with Fujian Monascus vinegar exhibiting the lowest
   elemental content. Fermentation methods (liquid-state vs. solid-state)
   significantly influenced mineral composition, with liquid-state
   fermentation resulting in lower mineral levels. Principal component
   analysis (PCA) revealed strong positive correlations among specific
   elements (e.g., B, K, Ca, Mg, Al, Mn, Se, Cd, Ba, Mo, Co, and Ni),
   suggesting their primary origin from raw grain ingredients. Both linear
   discriminant analysis (LDA) and partial least squares-discriminant
   analysis (PLS-DA) identified key classification markers, including Cd,
   B, Na, Pb, V, As, and Mo. LDA demonstrated superior machine learning
   performance, achieving classification accuracies of 96.7 \% for the
   training set and 90 \% for the validation set. A health risk assessment
   confirmed that daily vinegar consumption does not pose a heavy
   metalrelated health risk. This study establishes an effective
   methodology for vinegar authentication and confirms its safety,
   providing valuable insights for quality control and traceability.},
