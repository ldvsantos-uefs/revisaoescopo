
@article{ WOS:001428565200001,
Author = {Alonso-Salces, Rosa Maria and Viacava, Gabriela Elena and Tres, Alba and
   Vichi, Stefania and Valli, Enrico and Bendini, Alessandra and Toschi,
   Tullia Gallina and Gallo, Blanca and Berrueta, Luis Angel and Heberger,
